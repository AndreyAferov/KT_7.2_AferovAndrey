<Window x:Class="MasterFloor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MasterFloor"
        mc:Ignorable="d"
        Title="ИС - Мастер пол" Height="650" Width="1000"
        WindowStartupLocation="CenterScreen"
        MinHeight="650"
        MinWidth="1000">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#F4E8D3">
            <Image Source="/Resources/Мастер пол.png" HorizontalAlignment="Left" Margin="210,0,0,0" />
            <Label Content="Мастер пол" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="35"></Label>
        </Grid>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center"></Frame>
    </Grid>
</Window>
